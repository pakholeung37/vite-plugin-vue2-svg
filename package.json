{
  "name": "unplugin-svg-vue-component",
  "version": "0.0.5-beta.0",
  "description": "Use svg as vue components with support for both vue2.7 and vue3.x",
  "author": "pakholeung37@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/jaw52/unplugin-svg-vue-component",
  "keywords": [
    "vite",
    "svg-plugin",
    "vite-plugin",
    "vite-svg",
    "vue2",
    "vue3",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "svg.d.ts"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup",
    "typecheck": "vue-tsc --noEmit",
    "release": "bumpp && npm publish",
    "dev": "tsup --watch src",
    "play:vue2": "pnpm run -C ./examples/vite-vue2 dev",
    "play:vue3": "pnpm run -C ./examples/vite-vue3 dev",
    "build:fix": "esno scripts/postbuild.ts"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.2.25",
    "vue-template-compiler": "^2.7.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/component-compiler-utils": "^3.3.0",
    "local-pkg": "^0.4.3",
    "svgo": "^3.0.2",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/node": "^18.15.12",
    "bumpp": "^9.1.0",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "eslint": "^8.38.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "kolorist": "^1.7.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.2",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.2.0"
  }
}
